<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.dgit.library_project.dao.PostDao">
	<resultMap type="Post" id="PostResult">
		<result property="sido" column="sido"/>
		<result property="sigungu" column="sigungu"/>
	</resultMap>
	<sql id="selectBysigunguSql">
		select DISTINCT sigungu from post where sido = #{sido}
	</sql>
	<select id="selectBysigunguList" parameterType="Post" resultMap="PostResult">
		<include refid="selectBysigunguSql"/>
	</select>
	<sql id="selectBysidoSql">
		select DISTINCT sido from post
	</sql>
	<select id="selectBysidoList" resultMap="PostResult">
		<include refid="selectBysidoSql" />
	</select>
</mapper>